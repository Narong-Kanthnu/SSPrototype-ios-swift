# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

# Comment the next line if you're not using Swift and don't want to use dynamic frameworks
use_frameworks!

# Pods for SSPrototype
def installing_pods

    # SwiftGen
    pod 'SwiftGen', '~> 5.3.0'
    
    #pod Alamofire
    pod 'Alamofire'

    #pod ObjectMapper
    pod 'ObjectMapper'

    #pod Kingfisher
    pod 'Kingfisher'

    # Spring
    pod 'Spring', :git => 'https://github.com/MengTo/Spring.git'
    
    # SwiftyJSON
    pod 'SwiftyJSON'

    # UIImageColors
    pod "UIImageColors"

    # MarqueeLabel
    pod 'MarqueeLabel/Swift'
    
end

target 'SSPrototype' do
    installing_pods
end
